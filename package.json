  "lint-staged": {
    "*.{js,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@backtrack/core": "^0.1.18"
  },
  "dependencies": {
    "@backtrack/jest-serializer-preset": "^0.1.1",
    "@backtrack/preset-jest": "^0.2.0",
    "@backtrack/preset-style": "^0.1.5",
    "eslint-plugin-node": "^6.0.1",
    "read-pkg-up": "^3.0.0"
